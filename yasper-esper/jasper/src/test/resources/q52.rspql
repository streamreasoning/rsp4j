PREFIX ars: <http://www.streamreasoning/artist#>
PREFIX afn: <http://jena.apache.org/ARQ/function#>
REGISTER RSTREAM <out> AS
SELECT  ?a ?age ?uuid
FROM NAMED WINDOW <win2> ON <http://differenthost:12134/stream2> [RANGE PT5S STEP PT0.5S]
WHERE  {
    WINDOW ?w {
        ?a a ars:Artist ;
           ars:hasAge ?age .
    }

     BIND( UUID() as ?uuid )
}